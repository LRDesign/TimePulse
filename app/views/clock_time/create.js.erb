$('#timeclock').replaceWith("<%= escape_javascript(render :partial => 'shared/timeclock') %>");
$('#headline').html("<%= escape_javascript(project_headline(current_project)) %>");
$('#project_picker #project_<%= @project.id %>').addClass('current');
<% if @prior_project%>
  $('#project_picker #project_<%= @prior_project.id %>').removeClass('current');
<% end %>
